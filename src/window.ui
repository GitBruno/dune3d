                <property name="margin-top">4</property>
                <property name="width-chars">12</property>
                <property name="xalign">0.0</property>
                <style/>
              </object>
            </child>
            <child>
              <object class="GtkLabel" id="workplane_label">
                <property name="label">workplane goes here</property>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
