          <object class="GtkMenuButton" id="hamburger_menu_button">
            <property name="icon-name">open-menu-symbolic</property>
          </object>
        </child>
        <child type="start">
          <object class="GtkButton" id="new_button">
            <property name="icon-name">document-new-symbolic</property>
          </object>
        </child>
        <child type="end">
          <object class="GtkBox">
            <property name="css-classes">linked</property>
            <child>
              <object class="GtkButton" id="save_button">
                <property name="label">Save</property>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="save_as_button">
                <property name="icon-name">document-save-as-symbolic</property>
              </object>
            </child>
          </object>
        </child>
      </object>
    </property>
    <child>
      <object class="GtkBox">
        <property name="orientation">vertical</property>
        <child>
          <object class="GtkPaned" id="paned">
            <property name="position">300</property>
            <property name="position-set">True</property>
            <property name="resize-end-child">False</property>
            <property name="shrink-start-child">False</property>
            <property name="start-child">left_bar</property>
